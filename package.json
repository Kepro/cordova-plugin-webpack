{
  "name": "cordova-plugin-webpack",
  "version": "0.4.7",
  "description": "Cordova Webpack Plugin",
  "cordova": {
    "id": "cordova-plugin-webpack",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "scripts": {
    "upgrade-interactive": "npm-check --update",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "build": "rimraf scripts/* && tsc && webpack",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "commit": "commit",
    "version": "cordova-set-version plugin.xml && git add plugin.xml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kotarella1110/cordova-plugin-webpack.git"
  },
  "keywords": [
    "cordova",
    "webpack",
    "ecosystem:cordova",
    "cordova-browser",
    "cordova-android",
    "cordova-ios"
  ],
  "author": "Kotaro Sugawara",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kotarella1110/cordova-plugin-webpack/issues"
  },
  "homepage": "https://github.com/kotarella1110/cordova-plugin-webpack#readme",
  "dependencies": {
    "@types/elementtree": "^0.1.0",
    "@types/node": "^12.7.5",
    "@types/react-dev-utils": "^9.0.1",
    "@types/supports-color": "^5.3.0",
    "@types/webpack": "^4.39.1",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/yargs": "^13.0.2",
    "cordova-common": "^3.2.0",
    "create-html": "^4.1.0",
    "csp-parse": "0.0.2",
    "current-device": "^0.8.2",
    "elementtree": "^0.1.7",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "is-ua-webview": "^1.0.1",
    "react-dev-utils": "^9.0.3",
    "source-map-support": "^0.5.13",
    "supports-color": "^7.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-inject-plugin": "^1.5.3",
    "yargs": "^14.0.0",
    "yargs-unparser": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cordova-set-version": "^8.1.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
